# Optimizations: Brotli/Gzip + Cache + Redirect cleanup
<IfModule mod_brotli.c>
  BrotliCompressionLevel 6
  AddOutputFilterByType BROTLI_COMPRESS text/plain text/html text/xml text/css text/javascript application/javascript application/json image/svg+xml font/ttf font/otf
</IfModule>
<IfModule mod_deflate.c>
  AddOutputFilterByType DEFLATE text/plain text/html text/xml text/css text/javascript application/javascript application/json image/svg+xml font/ttf font/otf
</IfModule>
<IfModule mod_expires.c>
  ExpiresActive On
  ExpiresByType text/css "access plus 30 days"
  ExpiresByType application/javascript "access plus 30 days"
  ExpiresByType image/webp "access plus 90 days"
  ExpiresByType image/avif "access plus 90 days"
  ExpiresByType image/png "access plus 90 days"
  ExpiresByType image/jpeg "access plus 90 days"
  ExpiresByType image/svg+xml "access plus 90 days"
  ExpiresDefault "access plus 7 days"
</IfModule>
<FilesMatch "\.(js|css)$">
  Header set Cache-Control "public, max-age=2592000, immutable"
</FilesMatch>
<FilesMatch "\.(png|jpg|jpeg|webp|avif|svg)$">
  Header set Cache-Control "public, max-age=7776000, immutable"
</FilesMatch>
FileETag None
Header unset ETag
